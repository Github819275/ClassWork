# print(Unknown(10, 15))
# print('')
# print(10, "and", 10)
# print(Unknown(10, 10))
# print('')
# print(15, "and", 10)
# print(Unknown(15, 10))

print(10, "and", 15)
print(IterativeUnknown(10, 15))
print('')
print(10, "and", 10)
print(IterativeUnknown(10, 10))
print('')
print(15, "and", 10)
print(IterativeUnknown(15, 10))
