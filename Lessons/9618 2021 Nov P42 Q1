def Unknown(x, y):
    if x < y:
        print(x + y)
        return Unknown(x + 1, y) * 2
    else:
        if x == y:
            return 1
        else:
            print(x + y)
            return Unknown(x - 1, y) // 2

def IterativeUnknown(x, y):
    answer = 1
    while x != y:
        if x < y:
            print(x + y)
            x = x + 1
            answer = answer * 2
            x = x + 1
        else:
            print(x + y)
            x = x - 1
            answer = answer // 2
    return answer


# Main Program
